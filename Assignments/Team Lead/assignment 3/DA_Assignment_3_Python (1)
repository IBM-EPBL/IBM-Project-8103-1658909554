      "source": [
        "value =\"This dog runs faster than the other dog dude!\";\n",
        "\n",
        "def countdogs(value):\n",
        " count=0\n",
        "\n",
        " for word in value.lower().split(): \n",
        "   if word == 'dog' or word =='dogs':\n",
        "\n",
        "     count= count +1 \n",
        "     print(count)\n",
        "\n",
        "countdogs(value)\n"
      ]
    },
